public class MergeSort {
	// divide and conquer algo.
	void mergeSort(int numbers[]){
		if(numbers.length > 1){
			int mid = numbers.length/2;
			int tempArray[] = Arrays.copyOfRange(numbers, 0, mid - 1);
			mergeSort(numbers[mid]);
			System.out.println(n);
		}
	}

}
